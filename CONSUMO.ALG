Algoritmo "consumo"



Var

combustivel, consumo, distancia : real

Inicio

      escreva ("Digite a distância percorrida: ")
      leia (distancia)
      escreva ("Digite a quantida de combustível gasto: ")
      leia (combustivel)
      
      consumo <- distancia / combustivel
      
      escrevaL ("O consumo médio do veículo é ", consumo:1:3," km por litro")

Fimalgoritmo